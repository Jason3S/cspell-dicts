{
    "version": "0.2",
    "language": "en",
    "files": [
        "*.{md,txt,yaml,yml}",
        ".github/**/*.{md,txt,yaml,yml}",
        "dictionaries/[a-e]*/**/*.{md,txt,yaml,yml,js}",
        "LICENSE"
    ],
    "words": [
        "ALLCAPS",
        "appname",
        "autobuild",
        "codeql",
        "configstore",
        "dependabot",
        "django",
        "django's",
        "issuehunt",
        "liberapay",
        "otechie",
        "patreon",
        "streetsidesoftware",
        "tidelift"
    ],
    "flagWords": [],
    "maxNumberOfProblems": 1000,
    "ignorePaths": [
        ".vscode/**",
        "**/.git/**",
        "**/*.aff",
        "**/*.dic",
        "**/node_modules/**"
    ],
    "import": [],
    "overrides": [
        {
            "filename": "**/nl_NL/**",
            "language": "en,nl"
        },
        {
            "filename": "**/fr_FR/**/*.txt",
            "language": "en,fr"
        },
        {
            "filename": "**/da_DK/**",
            "language": "en,da"
        },
        {
            "filename": "**/de_DE/**",
            "language": "en,de"
        },
        {
            "filename": "**/en_GB/**",
            "language": "en_GB"
        },
        {
            "filename": "**/ru_RU/samples/**",
            "language": "ru,en"
        },
        {
            "filename": "**/sv/**",
            "language": "en,sv"
        }
    ]
}
