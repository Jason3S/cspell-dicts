{
    "version": "0.2",
    "language": "en",
    "files": [
        "*.{md,txt,yaml,yml}",
        ".github/**/*.{md,txt,yaml,yml}",
        "dictionaries/[a-e]*/**/*.{md,txt,yaml,yml,js}",
        "dictionaries/hr*/**/*.{md,txt,yaml,yml,js}",
        "dictionaries/latex/**/*.{md,txt,yaml,yml,js,tex}",
        "LICENSE"
    ],
    "words": [
        "ALLCAPS",
        "appname",
        "autobuild",
        "codeql",
        "configstore",
        "crowdsourcing",
        "dependabot",
        "django",
        "django's",
        "issuehunt",
        "liberapay",
        "otechie",
        "patreon",
        "streetsidesoftware",
        "tidelift"
    ],
    "flagWords": [],
    "maxNumberOfProblems": 1000,
    "ignorePaths": [
        ".vscode/**",
        "**/.git/**",
        "**/*.aff",
        "**/*.dic",
        "dictionaries/*/src/**",
        "**/node_modules/**"
    ],
    "import": [],
    "overrides": [
        {
            "filename": "**/nl_NL/**",
            "language": "en,nl"
        },
        {
            "filename": "**/ru_RU/samples/**",
            "language": "ru,en"
        },
        {
            "filename": "**/sv/**",
            "language": "en,sv"
        }
    ]
}
